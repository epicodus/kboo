<!DOCTYPE html>
<html>
  <head>
    <title>Kboo</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class='wrapper'>
      <%= render 'layouts/header' %>
      <%= render 'layouts/jplayer' %>
      <%= render 'layouts/on_air_next' %>
      <%= flash[:notice] %>
      <%= flash[:alert] %>
      <%= flash[:success] %>
      <%= yield %>
      <%= render 'layouts/footer' %>
    </div>
  </body>
</html>
