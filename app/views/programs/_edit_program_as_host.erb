<h3 class='form-header'>Edit <%= @program.title %></h3>
<div class='form'>
  <%= form_for @program do |f| %>
    <div class='form-group'>
      <%= f.label      :title %>
      <%= f.text_field :title, autofocus: true, class: 'form-control' %>
    </div>
    <div class='form-group'>
      <%= f.label      :description %>
      <%= f.text_area  :descriptionm, rows:3, class: 'form-control' %><br>
    </div>
    <div class='form-group'>
      <%= f.submit 'Submit', class: 'btn btn-success' %>
    </div>
  <% end %> 
</div>
    