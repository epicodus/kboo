<% program_highlights.highlights.each do |program| %>
  <div class='panel'>
    <div class='info-wrapper'>
      <span class='info-block'>
        <h2><%= link_to program[:title], program[:url], target: '_blank' %></h2>
      </span>   
<% end %>
<% program_schedules.programs.each do |program| %>
    <div>
        <h2>
          <% ProgramSchedules.instance[:programs].each do |program| %>
          <%= p program[:title] %>
          <% end %>
        </h2>
    </div>   
<% end %>

